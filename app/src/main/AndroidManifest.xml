            android:parentActivityName="es.unizar.eina.products.Productos.ListaProductos.ShoppingListEdit" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="es.unizar.eina.products.Productos.ListaProductos.ShoppingListEdit" />
        </activity>
    </application>

</manifest>
