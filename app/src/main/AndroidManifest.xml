    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="es.unizar.eina.products.Productos.Productos.Products"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="es.unizar.eina.products.Productos.ListaProductos.ShoppingList"
            android:label="@string/shopping_list_name" >
        </activity>
        <activity
            android:name="es.unizar.eina.products.Productos.Productos.ProductEdit"
            android:label="@string/title_activity_product_edit"
